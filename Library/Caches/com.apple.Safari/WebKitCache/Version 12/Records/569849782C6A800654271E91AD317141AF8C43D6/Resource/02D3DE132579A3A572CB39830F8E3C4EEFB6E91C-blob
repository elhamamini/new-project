<style>
    /*Strip the ul of padding and list styling*/
    div#menu-top ul {
        list-style-type:none;
        margin:0;
        padding:0;
        top: 0;
        position: absolute;
    }

    /*Create a horizontal list with spacing*/
    div#menu-top li {
        display:inline-block;
        float: left;			
    }

    /*Style for menu links*/
    div#menu-top li a {
        display:block;
        min-width:140px;
        height: 50px;
        text-align: center;
        line-height: 50px;
        font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
        color: #fff;
        background: #2f3036;
        text-decoration: none;
    }

    /*Hover state for top level links*/
    div#menu-top li:hover a {
        background: #19c589;
    }

    /*Style for dropdown links*/
    div#menu-top li:hover ul a {
        background: #f3f3f3;
        color: #2f3036;
        height: 40px;
        line-height: 40px;
    }

    /*Hover state for dropdown links*/
    div#menu-top li:hover ul a:hover {
        background: #19c589;
        color: #fff;
    }

    /*Hide dropdown links until they are needed*/
    div#menu-top li ul {
        display: none;
    }

    /*Make dropdown links vertical*/
    div#menu-top li ul li {
        display: block;
        float: none;
    }

    /*Prevent text wrapping*/
    div#menu-top li ul li a {
        width: auto;
        min-width: 100px;
        padding: 0 20px;
    }

    /*Display the dropdown on hover*/
    div#menu-top ul li a:hover + .hidden, .hidden:hover {
        display: block;
    }

    /*Style 'show menu' label button and hide it by default*/
    div#menu-top .show-menu {
        font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
        text-decoration: none;
        color: #fff;
        background: #2f3036;
        text-align: center;
        padding: 5px 0;
        display: none;
    }

    /*Hide checkbox*/
    div#menu-top input[type=checkbox]{
        display: none;
        -webkit-appearance: none;
    }

    /*Show menu when invisible checkbox is checked*/
    div#menu-top input[type=checkbox]:checked ~ #menu{
        display: block;
    }


    /*Responsive Styles*/

    @media screen and (max-width : 11760px){
        /*Make dropdown links appear inline*/
        div#menu-top ul {
            position: static;
            display: none;
        }
        /*Create vertical spacing*/
        div#menu-top li {
            margin-bottom: 1px;
        }
        /*Make all menu links full width*/
        div#menu-top ul li, li a {
            width: 100%;
        }
        /*Display 'show menu' link*/
        div#menu-top .show-menu {
            display:block;
        }
    }
</style>
